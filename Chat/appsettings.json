{
  "MongoDB": {
    "ConnectionString": "%MONGODB_CONNECTION_STRING%"
  },
  "Jwt": {
    "Key": "%JWT_KEY%",
    "Issuer": "%JWT_ISSUER%",
    "Audience": "%JWT_AUDIENCE%",
    "ExpiresInMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
